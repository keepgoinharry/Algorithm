#include <bits/stdc++.h>

using namespace std;
using ll = long long;
using pi=pair<int,int>;


int main() {
  ios::sync_with_stdio(false);
  cin.tie(0);
  int N;
  cin>>N;
  vector<int> v;
  for(int i=0; i<N; i++)
    {
      int tmp;
      cin>>tmp;
      v.push_back(tmp);
    }
  int B,C;
  cin>>B>>C;
  ll answer = 0;
  for(int i=0; i<v.size(); i++)
    {
      int num = v[i];
      num-=B;
      answer++;
      if(num>0)
      {
        answer += (num+C-1)/C;
      }
    }
  cout<<answer;
}
