#include <bits/stdc++.h>
using namespace std;
using ll = long long;

int N;
vector<vector<int>> v;
vector<vector<ll>> dp;

int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);
    int T;
    cin>>T;
    while(T--)
      { 
        cin>>N;
        v = vector<vector<int>>(2,vector<int>(N,0));
        dp = vector<vector<ll>>(N,vector<ll>(2,0));
        for(int i=0; i<2; i++)
          {
            for(int j=0; j<N; j++)
              {
                cin>>v[i][j];
              }
          }
        dp[0][0] = 0;
        dp[0][1] = v[0][0];
        dp[0][2] = v[1][0];
        for(int i=1; i<N; i++)
          {
            dp[i][0] = max({dp[i-1][0],dp[i-1][1],dp[i-1][2]});
            dp[i][1] = max(dp[i-1][0], dp[i-1][2])+v[0][i];
            dp[i][2] = max(dp[i-1][0], dp[i-1][1])+v[1][i];
          }
        ll answer;
        answer = max({dp[N-1][0],dp[N-1][1],dp[N-1][2]});
        cout<<answer<<endl;
      
    }
}
