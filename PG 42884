#include <string>
#include <vector>
#include <bits/stdc++.h>
using namespace std;

bool cmp(vector<int>&a, vector<int>&b)
{
    return a[1]<b[1];
}

int solution(vector<vector<int>> routes) {
    int answer = 1;
    sort(routes.begin(),routes.end(),cmp);
    int last = routes[0][1];
    for(int i=1; i<routes.size(); i++)
    {
        int nxt = routes[i][0];
        if(nxt>last)
        {
            answer++;
            last = routes[i][1];   
        }
    }
    return answer;
}
