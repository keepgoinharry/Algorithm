#include <bits/stdc++.h>
using namespace std;

struct cmp {
    bool operator()(int a, int b) {
        if(abs(a) == abs(b)) return a > b; // 절댓값 같으면 작은 수 우선
        return abs(a) > abs(b);            // 절댓값 작은 수 우선
    }
};

int main(int argc, char* argv[]) {
  ios::sync_with_stdio(false);
  cin.tie(nullptr);
  
  int N;
  cin>>N;
  
  priority_queue<int,vector<int>,cmp> pq;
  
  for(int i=0; i<N; i++)
  {
    int tmp;
    cin>>tmp;
    if(tmp!=0)
    {
        pq.push(tmp);
    }
    else
    {
      if(pq.empty())
      {
        cout<<0<<"\n";
      }
      else
      {
        cout<<pq.top()<<"\n";
        pq.pop();
      }
    }


  }
}
