#include <bits/stdc++.h>
using namespace std;
using ll = long long;

int INF=1e9;
int dx[4]={0,0,-1,1};
int dy[4]= {1,-1,0,0};

void bfs(int x, int y, vector<vector<int>>& dist,int& num)
{
  num++;
  dist[x][y] = 0;
  queue<pair<int,int>> q;
  q.push({x,y});
  while(!q.empty())
    {
      int cx = q.front().first;
      int cy = q.front().second;
      q.pop();
      
      for(int i=0; i<4; i++)
        {
          int nx = cx + dx[i];
          int ny = cy + dy[i];
          if(nx>=dist.size() || ny>=dist[0].size() || nx<0||ny<0)continue;
          if(dist[nx][ny]==1)
          {
            num++;
            dist[nx][ny] = 0;
            q.push({nx,ny});
          }
        }
    }
}
int main() {
  ios::sync_with_stdio(false); // C 표준 입출력과 동기화 끔
  cin.tie(nullptr);
  int N;
  cin>>N;
  vector<vector<int>> dist(N,vector<int>(N,0));
  for(int i=0; i<N; i++)
    {
      string s;
      cin>>s;
      for(int j=0; j<s.size(); j++)
        {
          dist[i][j] = s[j] - '0';
        }
    }
  int cnt = 0;
  vector<int> v; 
  for(int i=0; i<N; i++)
    {
      for(int j=0; j<N; j++)
        {
          if(dist[i][j] == 1)
          {
            cnt++;
            int n = 0;
            bfs(i,j,dist,n);
            v.push_back(n);
          }
        }
    }

  sort(v.begin(),v.end());
  cout<<cnt<<endl;
  for(auto i:v)cout<<i<<endl;
}


