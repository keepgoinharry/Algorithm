#include <bits/stdc++.h>
using namespace std;
using ll = long long;
int main() {
  ios::sync_with_stdio(false); // C 표준 입출력과 동기화 끔
  cin.tie(nullptr);
  int N,M;
  cin>>N>>M;
  vector<ll> p;
  p.push_back(0);
  long long num = 0;
  for(int i=0; i<N; i++)
    {
      int tmp;
      cin>>tmp;
      num+=tmp;
      p.push_back(num);
    }

 
  
  
  for(int i=0; i<M; i++)
    {
      int a,b;
      cin>>a>>b;
      cout<<p[b] - p[a-1]<<"\n";
      
    }
  
}

