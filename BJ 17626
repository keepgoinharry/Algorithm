#include <bits/stdc++.h>
using namespace std;
using ll = long long;
int main() {
  ios::sync_with_stdio(false); // C 표준 입출력과 동기화 끔
  cin.tie(nullptr);
  int N;
  cin>>N;
  int cnt = 0;
  
  vector<int> dp(50001);
  dp[1]=1;
  dp[2]=2;
  dp[3]=3;
  dp[4]=1;
  for(int i=5; i<50001; i++)
    {
      int num = 99999999;
      for(int j=1; j*j<=i; j++)
        {
          num = min(dp[i-j*j],num);
        }
      dp[i] = num+1;
    }
  //for(int i=1; i<20; i++)cout<<dp[i]<<" ";
  cout<<dp[N];
}

