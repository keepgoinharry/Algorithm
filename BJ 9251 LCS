#include <bits/stdc++.h>

using namespace std;
using ll = long long;
using pi=pair<int,int>;

int main() {
  ios::sync_with_stdio(false);
  cin.tie(0);
  string X,Y;
  cin>>X;
  cin>>Y;
  int xsize = X.size();
  int ysize = Y.size();
  vector<vector<int>> dp(xsize+1, vector<int>(ysize+1,0));

  for(int i=1; i<=xsize; i++)
    {
      for(int j=1; j<=ysize; j++)
        {
          cout<<i<<" "<<j<<endl;
          if(X[i-1]==Y[j-1])
          {
            dp[i][j] = dp[i-1][j-1] + 1;
          }
          else
          {
            dp[i][j] = max(dp[i-1][j],dp[i][j-1]);
          }
        }
    }
  cout<<dp[xsize][ysize];
}
