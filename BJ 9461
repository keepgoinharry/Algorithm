#include <bits/stdc++.h>
using namespace std;

int main() {
  ios::sync_with_stdio(false); // C 표준 입출력과 동기화 끔
  cin.tie(nullptr);
  int T;
  cin>>T;
  vector<long long> v(101);
  v[1]=1;
  v[2]=1;
  v[3]=1;
  for(int i=4; i<101; i++)
    {
      v[i] = v[i-3]+v[i-2];
    }
  for(int i=0; i<T; i++)
    {
      int num;
      cin>>num;
      cout<<v[num]<<"\n";
        
    }

  
}

