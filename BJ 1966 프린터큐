#include <bits/stdc++.h>
using namespace std;
using ll = long long;

int main() {
  ios::sync_with_stdio(false); // C 표준 입출력과 동기화 끔
  cin.tie(nullptr);
  
  int n;
  cin>>n;
  stack<int> st;
  vector<int> v;
  int idx = 0;
  for(int i=1; i<=n; i++)
    {
      int tmp;
      cin>>tmp;
      v.push_back(tmp);
    }
  vector<char> ans;
  for(int i=1; i<=n; i++)
    {
      while(!st.empty()&&st.top()==v[idx])
      {
        st.pop();
        ans.push_back('-');
        idx++;
      }
      st.push(i);
      ans.push_back('+');
    }

  while(!st.empty()&&st.top()==v[idx])
    {
      st.pop();
      ans.push_back('-');
      idx++;
    }
  if(st.empty())
    for(auto i:ans)cout<<i<<"\n";
  else cout<<"NO";
}
