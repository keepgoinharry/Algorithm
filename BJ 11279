#include <bits/stdc++.h>
using namespace std;
using ll = long long;
int main() {
  ios::sync_with_stdio(false); // C 표준 입출력과 동기화 끔
  cin.tie(nullptr);
  priority_queue<ll,vector<ll>,less<>> pq;
  int n;
  cin>>n;
  for(int i=0; i<n; i++)
    {
      ll tmp;
      cin>>tmp;
      if(tmp == 0)
      {
        if(!pq.empty())
        {
          cout<<pq.top()<<"\n";
          pq.pop();
        }
        else cout<<0<<"\n";
      }
      else
      {
        pq.push(tmp);
      }
    }
}

