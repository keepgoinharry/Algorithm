#include <bits/stdc++.h>
using namespace std;
using pi = pair<long long,long long>;

int main() {
    ios::sync_with_stdio(false);
    cin.tie(0);
    int N,K;
    cin>>N>>K;
    int answer = N;

    for(int i=2; i<=K; i++)
        {
            int num = N*i;
            string s = to_string(num);
            s=string(s.rbegin(),s.rend());
            answer = max(answer, stoi(s));
        }
    cout<<answer;
}
