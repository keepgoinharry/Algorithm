#include <string>
#include <vector>

using namespace std;

vector<int> solution(int n, int s) {
    vector<int> answer;
    if(s<n)
    {
        answer.push_back(-1);
        return answer;
    }
    
    answer = vector<int>(n,s/n);
    
    int r = s%n;
    
    int idx = answer.size() - 1;
    
    for(int i=0; i<r; i++)
    {
        answer[idx]++;
        idx--;
    }
    return answer;
}
