#include <bits/stdc++.h>
using namespace std;

int main() { 
  int N;
  cin>>N;
  vector<int> a(N,0),b(N);

  
  for(auto& i : b)cin>>i;
  int cnt = 0;
  while(true)
    {
      int zero = 0;
      for(auto& i:b)
        {
          if(i%2!=0)
          {
            i-=1;
            cnt++;
          }

          if(i==0)zero++;
        }
      if(zero == N)break;

      for(auto& i:b)
        {
          i/=2;
        }
      cnt++;
    }
  cout<<cnt;
}
