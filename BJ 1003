#include <bits/stdc++.h>
using namespace std;
using ll = long long;

bool is_number(const std::string& str) {
    return !str.empty() && std::all_of(str.begin(), str.end(), ::isdigit);
}

int main() {
  ios::sync_with_stdio(false); // C 표준 입출력과 동기화 끔
  cin.tie(nullptr);
  int T;
  cin>>T;
  vector<pair<int,int>> v(41);
  v[0]={1,0};
  v[1]={0,1};
  for(int i=2; i<=40; i++)
    {
      int a = v[i-1].first + v[i-2].first;
      int b = v[i-1].second + v[i-2].second;
      v[i] = {a,b};
    }
  for(int i=0; i<T; i++)
    {
      int tmp;
      cin>>tmp;
      cout<<v[tmp].first<<" "<<v[tmp].second<<"\n";
    }
  
}

