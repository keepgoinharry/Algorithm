#include <bits/stdc++.h>
using namespace std;
using ll = long long;

int main() {
    ios::sync_with_stdio(false);
    cin.tie(0);
    string s;
    getline(cin,s);
    //cin.ignore();

    string finds;
    getline(cin,finds);

    //cout<<finds;
    int size = finds.size();
    //cout<<size<<endl;
    int cnt = 0;
    for(int i=0; i+size<=s.size(); i++)
        {
            string tmp = s.substr(i,size);
            //cout<<tmp<<endl;
            //cout<<i<<endl;
            if(tmp==finds)
            {
                cnt++;
                i+=size-1;
            }
        }
    cout<<cnt;
}
