#include <bits/stdc++.h>
using namespace std;

int main() {
  ios::sync_with_stdio(false); // C 표준 입출력과 동기화 끔
  cin.tie(nullptr);
  int T;
  cin>>T;
  
  for(int i=0; i<T; i++)
    {
      int cnt;
      cin>>cnt;
      cin.ignore();
      map<string,int> m;
      for(int j=0; j<cnt; j++)
        {
            string s;
            getline(cin,s);
            stringstream ss(s);
            string word;
            while(ss>>word);
            m[word]++;
        }
      int answer = 1;
      for(auto i:m)answer*=i.second+1;
      cout<<answer-1<<"\n";
        
    }

  
}

