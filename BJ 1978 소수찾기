#include<iostream>
#include <string>
#include <vector>
#include <sstream>>
#include <map>
#include <algorithm>
#include <cmath>
using namespace std;
int main()
{ 
  int n;
  cin>>n;
  int cnt = 0;
  vector<int> v;
  for(int i=0; i<n; i++)
    {
      int tmp;
      cin>>tmp;
      v.push_back(tmp);
    }
  for(int i=0; i<v.size(); i++)
    {
      bool flag = false;
      if(v[i]<=1)continue;
      for(int j=2; j<(v[i]); j++)
        {
          if(v[i]%j==0)flag=true;
        }
      if(!flag)cnt++;
    }
  cout<<cnt;
}
