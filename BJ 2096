#include <bits/stdc++.h>
using namespace std;
int inf = 1e9;
int main() {
  ios::sync_with_stdio(false);
  cin.tie(nullptr);
  int N;
  cin>>N;
  vector<int> prev_max(3),prev_min(3),
              cur_max(3),cur_min(3);

  for(int i=0; i<N; i++)
    {
      int a,b,c;
      cin>>a>>b>>c;
      if(i==0)
      {
        prev_max[0]=a;
        prev_max[1]=b;
        prev_max[2]=c;
        
        prev_min[0]=a;
        prev_min[1]=b;
        prev_min[2]=c;
      }else{
        cur_max[0] = max(prev_max[0],prev_max[1]) + a;
        cur_max[1] = max({prev_max[0],prev_max[1],prev_max[2]}) + b;
        cur_max[2] = max(prev_max[2],prev_max[1]) + c;
        
        cur_min[0] = min(prev_min[0],prev_min[1]) + a;
        cur_min[1] = min({prev_min[0],prev_min[1],prev_min[2]}) + b;
        cur_min[2] = min(prev_min[2],prev_min[1]) + c;

        prev_max = cur_max;
        prev_min = cur_min;
      }
    }
  int mx = *max_element(prev_max.begin(),prev_max.end());
  int mn = *min_element(prev_min.begin(),prev_min.end());

  cout<<mx<<" "<<mn;
  
  


}
