#include <bits/stdc++.h>
using namespace std;
using ll = long long;

int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);
    int A,B;
    cin>>A>>B;
    queue<pair<ll,int>> q;
    q.push({A,1});
    int answer = -1;
    while(!q.empty())
    {
        ll cx = q.front().first;
        int cnt = q.front().second;
        q.pop();
        if(cx==B)
        {
            answer = cnt;
            break;
        }
        ll nx1 = cx*2;
        if(nx1<=B)
        {
            q.push({nx1,cnt+1});
        }
        ll nx2 = (cx*10) + 1;
        if(nx2<=B)
            {
                q.push({nx2,cnt+1});
            }
    }
    cout<<answer;
}
