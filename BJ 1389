#include <bits/stdc++.h>
using namespace std;
using ll = long long;

int main() {
  ios::sync_with_stdio(false); // C 표준 입출력과 동기화 끔
  cin.tie(nullptr);
  int N,M;
  cin>>N>>M;
  vector<vector<int>> graph(N+1);
  for(int i=0; i<M; i++)
    {
      int a,b;
      cin>>a>>b;
      graph[a].push_back(b);
      graph[b].push_back(a);
    }

  int num = 99999999;
  int answer = 0;
  
  for(int i=1; i<=N; i++)
    {
      vector<int> dist(N+1,-1);
      vector<bool> visited(N+1,0);
      dist[i]=0;
      queue<int> q;
      visited[i] = true;
      q.push(i);
      while(!q.empty())
        {
          int cur = q.front(); q.pop();
          for(int j=0; j<graph[cur].size(); j++)
            {
              int nx = graph[cur][j];
              if(!visited[nx])
              {
                visited[nx] = true;
                dist[nx] = dist[cur] + 1;
                q.push(nx);
              }
            }
        }
      int cnum = accumulate(dist.begin()+1,dist.end(),0);
      
      if(num > cnum)
      {
        num = cnum; 
        answer =i;
      }
      
    }
  cout<<answer;
}


