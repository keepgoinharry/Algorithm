#include <string>
#include <vector>
#include <bits/stdc++.h>
using namespace std;

long long solution(int n, vector<int> times) {
    long long answer = 0;
    long long s = 1;
    long long e = 1e18;
    
    while(s<=e)
    {
        long long mid = (s+e)/2;
        long long cnt = 0;
        for(int i=0; i<times.size(); i++)
        {
            cnt+=mid/times[i];
        }
        //cout<<s<<" "<<mid<<" "<<e<<" "<<cnt<<endl;
        if(cnt>=n)
        {
            answer = mid;
            e = mid - 1;
        }
        else if(cnt<n) s = mid+1;
        
    } 
    return answer;
}
