#include <bits/stdc++.h>
using namespace std;
using ll = long long;

map<char,pair<char,char>> m;

void preorder(char node)
{
  if(node=='.')return;
  cout<<node;
  preorder(m[node].first);
  preorder(m[node].second);
}

void inorder(char node)
{
  if(node=='.')return;
  inorder(m[node].first);
  cout<<node;
  inorder(m[node].second);
}

void postorder(char node)
{
  if(node=='.')return;
  postorder(m[node].first);
  postorder(m[node].second);
  cout<<node;
  
}

int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);
    int N;
    cin>>N;
    for(int i=0; i<N; i++)
      {
        char a,b,c;
        cin>>a>>b>>c;
        m[a].first = b;
        m[a].second = c;
      }

  preorder('A');
  cout<<endl;

  inorder('A');
  cout<<endl;

  postorder('A');
  cout<<endl;
  
}
